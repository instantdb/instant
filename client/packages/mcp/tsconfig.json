{
  "extends": "../../tsconfig.base.json",
  "include": ["src"],
  "exclude": ["node_modules", "dist"],
  "compilerOptions": {
    "strict": true,
    "outDir": "dist",
    "rewriteRelativeImportExtensions": true,
    "rootDir": "src",
    "checkJs": false,
    "skipLibCheck": true,
    "module": "Node16",
    "moduleResolution": "Node16",
    "target": "ES2022"
  }
}
