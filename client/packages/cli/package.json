{
  "name": "instant-cli",
  "type": "module",
  "version": "0.0.0",
  "description": "Instant's CLI",
  "homepage": "https://github.com/instantdb/instant/tree/main/client/packages/cli",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instantdb/instant.git",
    "directory": "client/packages/cli"
  },
  "exports": {
    "./ui": {
      "import": {
        "types": "./dist/ui/index.d.ts",
        "default": "./dist/ui/index.js"
      }
    }
  },
  "bin": {
    "instant-cli": "bin/index.js"
  },
  "dependencies": {
    "@inquirer/core": "9.0.10",
    "@inquirer/prompts": "5.3.8",
    "@instantdb/platform": "workspace:*",
    "@instantdb/version": "workspace:*",
    "ansi-escapes": "4.3.2",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.3.1",
    "dotenv-flow": "^4.1.0",
    "env-paths": "^3.0.0",
    "hanji": "^0.0.5",
    "inquirer": "^10.1.6",
    "json-diff": "^1.0.6",
    "lodash.throttle": "^4.1.1",
    "open": "^10.1.0",
    "ora": "^8.1.1",
    "pkg-dir": "^8.0.0",
    "prettier": "^3.3.3",
    "semver": "7.7.2",
    "strip-ansi": "^7.1.2",
    "sisteransi": "^1.0.5",
    "terminal-link": "^3.0.0",
    "unconfig": "^0.5.5"
  },
  "scripts": {
    "test": "vitest",
    "build": "rm -rf dist; tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch --skipLibCheck --preserveWatchOutput | grep -v '^$'",
    "publish-package": "pnpm publish --access public --no-git-checks"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@inquirer/type": "^3.0.2",
    "@types/json-diff": "^1.0.3",
    "@types/lodash.throttle": "^4.1.9",
    "@types/node": "^22.6.1",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.3"
  }
}
